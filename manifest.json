{
    "manifest_version": 3,
    "name": "Pants",
    "version": "0.1.0",
    "description": "Redirect YouTube shorts to YouTube videos",
    "icons": {
      "48": "icons/pants-icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["pants.js"]
      }
    ]
  }
